{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mirandawork/migrations"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel-doctrine/migrations": "dev-patch-1",
        "laravel-doctrine/orm": "1.4.*",
        "laravel/lumen-framework": "5.8.*"
    },
    "require-dev": {
        "darkaonline/swagger-lume": "5.6.*",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "zircote/swagger-php": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "OpenApi\\": "vendor/darkaonline/swagger-lume/src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
